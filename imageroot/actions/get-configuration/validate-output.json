{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Get Copyparty configuration",
  "$id": "Copyparty",
  "description": "Get Copyparty configuration",
  "examples": [
    {
      "username": "username",
      "password": "password",
      "http_port": 3923
    }
  ],
  "type": "object",
  "required": [
    "username",
    "password",
    "http_port"
  ],
  "properties": {
     "username": {
      "type": [
        "string",
        "null"
      ],
      "description": "Default username for copyparty user, it can be changed later"
    },
    "password": {
      "type": [
        "string",
        "null"
      ],
      "description": "Default password for copyparty user, it can be changed later"
    },
    "http_port": {
      "type": "integer",
      "title": "port",
      "description": "The port of the http service",
      "minimum": 1024,
      "maximum": 65535
    }
  }
}
